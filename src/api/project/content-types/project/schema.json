{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "vimeo_player_link": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "thumbnail_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "project_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project-category.project-category",
      "mappedBy": "projects"
    },
    "client": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "is_featured": {
      "type": "boolean"
    },
    "thumbnail_video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "position": {
      "type": "integer"
    },
    "non_vimeo_embed_code": {
      "type": "text"
    },
    "vimeo_player_link_mobile": {
      "type": "string"
    },
    "project_links": {
      "displayName": "Project link",
      "type": "component",
      "repeatable": true,
      "component": "project-page.project-link"
    },
    "description_continued": {
      "type": "richtext"
    },
    "slideshow": {
      "type": "dynamiczone",
      "components": [
        "project-page.slideshow-item-iframe",
        "project-page.slideshow-item-image"
      ]
    },
    "vimeo_player_link_square": {
      "type": "string"
    }
  }
}
